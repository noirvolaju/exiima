<h1>Listing posts</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Carrera</th>
    <th>Clase1</th>
    <th>Clase2</th>
    <th>Clase3</th>
    <th>Clase4</th>
    <th>Materia1</th>
    <th>Materia2</th>
    <th>Materia3</th>
    <th>Materia4</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%= post.nombre %></td>
    <td><%= post.carrera %></td>
    <td><%= post.clase1 %></td>
    <td><%= post.clase2 %></td>
    <td><%= post.clase3 %></td>
    <td><%= post.clase4 %></td>
    <td><%= post.materia1 %></td>
    <td><%= post.materia2 %></td>
    <td><%= post.materia3 %></td>
    <td><%= post.materia4 %></td>
    <td><%= link_to 'Show', post %></td>
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    <td><%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post', new_post_path %>
